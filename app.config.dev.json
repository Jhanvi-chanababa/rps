{
  "Name": "RPS Configuration File",
  "Description": "Contains settings to configure the RPS Server",
  "devmode": true,
  "VaultConfig": {
    "usevault": true,
    "SecretsPath": "secret/data/",
    "address": "http://localhost:8200",
    "token": "myroot"
  },
  "DbConfig": {
    "useDbForConfig": true,
    "dbhost": "localhost",
    "dbname": "postgresdb",
    "dbport": 5432,
    "dbuser": "<your_db_username>",
    "dbpassword": "<your_db_password>"
  },
  "mpsusername": "admin",
  "mpspass": "<your_mps_password>",
  "amtusername": "admin",
  "RPSXAPIKEY": "<your_mps_APIKEY>",
  "https": false,
  "webport": 8081,
  "credentialspath": "../../../mps/private/credentials.json",
  "WSConfiguration": {
    "WebSocketPort": 8080,
    "WebSocketTLS": false,
    "WebSocketCertificate": "tlscert.pem",
    "WebSocketCertificateKey": "tlskey.pem"
  },
  "AMTConfigurations": [
    {
      "ProfileName": "Sample_Profile",
      "AMTPassword": "<your_amt_password>",
      "GenerateRandomPassword": false,
      "RandomPasswordLength": 8,
      "Activation": "acmactivate"
    }
  ],
  "AMTDomains": [
    {
      "Name": "Sample_Domain",
      "DomainSuffix": "sample.com",
      "ProvisioningCert": "private/sample.pfx",
      "ProvisioningCertStorageFormat": "file",
      "ProvisioningCertPassword": "<your_certificate_password>"
    }
  ]
}
